{
  "apiVersion": "monitoring.coreos.com/v1",
  "kind": "Prometheus",
  "metadata": {
    "name": "prometheus"
  },
  "spec": {
    "serviceMonitorSelector": {
      "matchLabels": {
        "app": "openmatch"
      }
    },
    "serviceAccountName": "prometheus",
    "resources": {
      "requests": {
        "memory": "400Mi"
      }
    }
  }
}

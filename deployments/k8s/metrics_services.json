{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "om-frontend-metrics",
    "labels": {
      "app": "openmatch",
      "component": "frontend",
      "agent": "opencensus",
      "destination": "prometheus"
    }
  },
  "spec": {
    "selector": {
      "app": "openmatch",
      "component": "frontend"
    },
    "ports": [
      {
        "name": "metrics",
        "targetPort": 9555,
        "port": 19555
      }
    ]
  }
}
